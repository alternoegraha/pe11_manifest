  <project path="external/libavc" name="android_external_libavc" remote="lineage" groups="pdk" />
  <project path="external/libcups" name="android_external_libcups" remote="lineage" groups="pdk-cw-fs,pdk-fs" />
  <project path="external/libexif" name="android_external_libexif" remote="lineage" groups="pdk" />
  <project path="external/libvpx" name="android_external_libvpx" remote="lineage" groups="pdk" />
